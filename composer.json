{
    "name": "sepiphy/container",
    "description": "The \"sepiphy/container\" package.",
    "keywords": ["sepiphy", "tools", "phptools", "container"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/sepiphy/phptools",
    "support": {
        "issues": "https://github.com/sepiphy/phptools/issues",
        "source": "https://github.com/sepiphy/phptools"
    },
    "authors": [
        {
            "name": "Quynh Xuan Nguyen",
            "email": "seriquynh@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "sepiphy/contracts": "^0.7",
        "symfony/dependency-injection": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Sepiphy\\PHPTools\\Container\\": "./"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
